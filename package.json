{
  "name": "svelte-storybook-starter",
  "version": "1.0.0",
  "svelte": "src/index.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "check": "svelte-check",
    "build": "rollup -c",
    "dev": "npm run storybook",
    "start": "sirv storybook-static --no-clear",
