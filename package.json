    "lint": "prettier --check . && eslint --ext .cjs,.js,.ts,.json,.svelte .",
    "format": "prettier --write . && eslint --fix --ext .cjs,.js,.ts,.json,.svelte ."
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@rollup/plugin-commonjs": "^17.0.0",
