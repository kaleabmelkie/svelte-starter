{
  "name": "svelte-storybook-starter",
  "svelte": "src/index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "scripts": {
    "lint": "prettier --check src \"*.{json,js,cjs,ts,tsx,svelte}\" --ignore-path ./.gitignore --plugin-search-dir .",
    "format": "prettier --check --write src \"*.{json,js,cjs,ts,tsx,svelte}\" --ignore-path ./.gitignore --plugin-search-dir .",
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/addon-svelte-csf": "1.1.0",
    "@storybook/svelte": "6.4.19",
    "babel-loader": "8.2.4",
    "prettier": "2.6.0",
    "rollup": "2.70.1",
    "rollup-plugin-svelte": "7.1.0",
    "svelte": "3.46.4",
    "svelte-loader": "3.1.2"
  },
  "keywords": [
    "svelte"
  ],
  "files": [
    "src",
    "dist"
  ]
}
